curdir <- getwd()
setwd("../subst_func/")
for(i in dir()) source(i)
setwd(curdir)
fols <- grep("ML", dir(), value = T)
fols <- grep("folder", fols, value = T)

for(i in 1:length(fols)){
      setwd(fols[i])
      try(adeqres <- adeq.ml(grep("_phyml_tree", dir(), value = T), grep("_phyml_stat", dir(), value = T), grep("phy$", dir(), value = T), Nsim = 100))
      save(adeqres, file = "adeqres.Rdata")
      setwd("..")
}